// https://leetcode.com/contest/weekly-contest-359/problems/check-if-a-string-is-an-acronym-of-words/
class Solution {
    public boolean isAcronym(List<String> words, String s){
        String m = "";
        for(int i=0;i<words.size();i++){
            String temp = words.get(i);
            m = m + temp.charAt(0);
        }
            if(m.equals(s)){
                return true;
            }
        return false;
        }

}